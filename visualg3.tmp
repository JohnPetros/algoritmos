Algoritmo "estruturaderepiticao"

Var
    I, F: Inteiro


Inicio
   EscrevaL("CONTAGEM INTELIGENTE")
   EscrevaL("--------------------")
   Escreva("Início: ")
   Leia(I)
   Escreva("Fim: ")
   Leia(F)
   EscrevaL("--------------------")
   EscrevaL("  C O N T A N D O  ")
   EscrevaL("--------------------")
   Se (I < F) entao
      Enquanto (I<=F) faca
         Escreva(I, ".. ")
         I <- I + 1
      FimEnquanto
   FimSe



Fimalgoritmo