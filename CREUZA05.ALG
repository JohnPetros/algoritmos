Algoritmo "creuza05"

Var
   Emprest, QParcel, VParcel: Real


Inicio
      Escreva("Qual é o valor de empréstimo? R$")
      Leia(Emprest)
      Emprest <- Emprest * 1.2
      Escreva("Quantas parcelas? ")
      Leia(QParcel)
      VParcel <- Emprest / Qparcel
      Escreva("Vou pagar", QParcel, " parcelas de R$ ", VParcel:5:2)


Fimalgoritmo