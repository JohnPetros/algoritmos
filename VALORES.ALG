Algoritmo "valores"

Var
  N1, N2, S, M: Real

Inicio
      Escreva("Informe um número: ")
      Leia(N1)
      Escreva("Informe outro número: ")
      Leia(N2)
      S <- N1 + N2
      Escreval("A soma entre", N1, " e", N2, " é", S)
      M <- (N1 + N2) / 2
      Escreva("A média entre", N1, " e", N2, " é" , M)
      
      


Fimalgoritmo