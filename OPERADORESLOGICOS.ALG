Algoritmo "operadoreslogicos"

Var
   A, B, C: Inteiro


Inicio
      A <- 2
      B <- 3
      C <- 5
      EscrevaL(A>B)
      EscrevaL(A=B)
      EscrevaL(A<>B)
      EscrevaL(A>=2)
      EscrevaL(C=A+B)
      EscrevaL(C=A+B)
      EscrevaL(C<=B^A)
      EscrevaL(1=C%2)
      EscrevaL(verdadeiro e falso)
      EscrevaL((A=B) ou (C>A))
      EscrevaL(nao((A=B) ou (C>A)))
      


Fimalgoritmo